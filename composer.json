{
    "name": "jaguarjack/catchadmin",
    "description": "The api framework base on thinkphp, help you to create admin management soon ðŸš€",
    "type": "project",
    "keywords": [
        "framework",
        "catchadmin",
        "api",
        "ORM"
    ],
    "homepage": "https://catchadmin.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "jaguarjack",
            "email": "njphper@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "topthink/framework": "6.0.10",
        "topthink/think-orm": "2.0.49",
        "overtrue/wechat": "^4.2",
        "phpoffice/phpspreadsheet": "^1.12",
        "dragonmantank/cron-expression": "3.1",
        "symfony/finder": "^4.4",
        "ext-json": "*",
        "overtrue/easy-sms": "^1.1",
        "jaguarjack/migration-generator": "dev-master",
        "jaguarjack/think-filesystem-cloud": "1.0",
        "topthink/think-view": "^1.0",
        "xaboy/form-builder": "~2.0",
        "ext-zip": "*",
        "lcobucci/jwt": "^4.1",
        "catchadmin/phinx": "1.0.0"
    },
    "require-dev": {
        "topthink/think-trace": "^1.0",
        "symfony/var-dumper": "^5.1",
        "nikic/php-parser": "^4.10",
        "jaguarjack/file-generate": "dev-master",
        "fzaninotto/faker": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app",
            "catchAdmin\\": "catch",
            "catchAdmin\\jwt\\": "extend/jwt-auth/src/",
            "catchAdmin\\migration\\": "extend/think-migration/src/"
        },
        "psr-0": {
            "": "extend/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "catchAdmin\\tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    }
}
